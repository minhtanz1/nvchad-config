{
  "LaTeX Inline Math": {
    "prefix": "mi",
    "body": [
      "$$1$ $0"
    ],
    "description": "Môi trường toán học nội tuyến (inline math) dùng dấu $."
  },
  "LaTeX Equation Environment": {
    "prefix": "me",
    "body": [
      "\\begin{equation}",
      "\t$1",
      "\\end{equation}$0"
    ],
    "description": "Môi trường toán học hiển thị (equation environment)."
  },
  "LaTeX Aligned Environment": {
    "prefix": "maligned",
    "body": [
      "\\begin{aligned}",
      "\t$1 &= $2 \\\\",
      "\t$3 &= $4",
      "\\end{aligned}$0"
    ],
    "description": "Môi trường căn chỉnh công thức (aligned environment)."
  },
  "LaTeX Cases Environment": {
    "prefix": "mcases",
    "body": [
      "\\begin{cases}",
      "\t$1 & \\text{if } $2 \\\\",
      "\t$3 & \\text{if } $4",
      "\\end{cases}$0"
    ],
    "description": "Môi trường hàm từng phần (cases environment)."
  },
  "Fraction": {
    "prefix": "mf",
    "body": [
      "\\frac{$1}{$2}$0"
    ],
    "description": "Phân số (\\frac)."
  },
  "Square Root": {
    "prefix": "msq",
    "body": "\\sqrt{$1}$0",
    "description": "Căn bậc hai (\\sqrt)."
  },
  "Nth Root": {
    "prefix": "mnth",
    "body": "\\sqrt[$1]{$2}$0",
    "description": "Căn bậc n (\\sqrt[n])."
  },
  "Summation (Sigma)": {
    "prefix": "msum",
    "body": "\\sum_{$1}^{$2} $3$0",
    "description": "Tổng (\\sum)."
  },
  "Integral": {
    "prefix": "mint",
    "body": "\\int_{$1}^{$2} $3 d$4$0",
    "description": "Tích phân (\\int)."
  },
  "Limit": {
    "prefix": "mlim",
    "body": "\\lim_{$1 \\to $2} $3$0",
    "description": "Giới hạn (\\lim)."
  },
  "Partial Derivative": {
    "prefix": "mpartial",
    "body": "\\frac{\\partial $1}{\\partial $2}$0",
